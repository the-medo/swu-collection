{
  "name": "swu-collection",
  "module": "server/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun server/index.ts",
    "dev": "bun --watch server/index.ts",
    "auth-generate": "npx @better-auth/cli generate --config server/auth/auth.ts",
    "db-generate": "bun drizzle-kit generate",
    "db-migrate": "bun migrate.ts",
    "db-docker": "docker run -d --name swubase-postgres -e POSTGRES_PASSWORD=password -p 5442:5432 postgres:16-alpine"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.34",
    "@types/bun": "^1.3.1",
    "drizzle-kit": "^0.31.6",
    "prettier": "^3.4.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.920.0",
    "@hono/zod-validator": "^0.7.4",
    "@sentry/bun": "^10.22.0",
    "better-auth": "^1.3.34",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.3",
    "linkedom": "^0.18.12",
    "postgres": "^3.4.5",
    "sharp": "^0.34.4",
    "zod": "^4.1.12"
  }
}