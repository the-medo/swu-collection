    "better-auth": "^1.3.34",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.3",
    "linkedom": "^0.18.12",
    "postgres": "^3.4.5",
    "sharp": "^0.34.4",
